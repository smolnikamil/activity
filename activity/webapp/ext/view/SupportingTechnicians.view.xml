<mvc:View xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:m="sap.m"
	xmlns="sap.ui.table"
	xmlns:html="http://www.w3.org/1999/xhtml" controllerName="fsm.connector.activity.ext.controller.SupportingTechnicians">

	<Table id="TableSupportingTechnicians" rows="{path:'toSupportingTechnicians'}" rowMode='Auto' selectionMode="None">
		<columns>
			<Column id="ColumnSupportingTechniciansPerson">
				<m:Label id="LabelSupportingTechniciansPerson" text="{i18n>Person}"/>
				<template>
					<m:Text id="TextSupportingTechniciansPerson" text="{SupportingPerson}" wrapping="false" />
				</template>
			</Column>
			<Column id="ColumnSupportingTechniciansName">
				<m:Label id="LabelSupportingTechniciansName" text="{i18n>Name}"/>
				<template>
					<m:Text id="TextSupportingTechniciansName" text="{SupportingPersonName}" wrapping="false" />
				</template>
			</Column>
		</columns>
	</Table>

</mvc:View>